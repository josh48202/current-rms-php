{
    "name": "wjbecker/current-rms-php",
    "description": "A framework-agnostic PHP library for the Current RMS API with fluent query builder, pagination, and collections",
    "keywords": [
        "current-rms",
        "api",
        "client",
        "rental",
        "inventory",
        "query-builder",
        "pagination",
        "collection"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "William Becker",
            "email": "josh48202@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "pestphp/pest": "^3.0|^4.0",
        "mockery/mockery": "^1.6",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Wjbecker\\CurrentRms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wjbecker\\CurrentRms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wjbecker\\CurrentRms\\CurrentRmsServiceProvider"
            ],
            "aliases": {
                "CurrentRms": "Wjbecker\\CurrentRms\\Facades\\CurrentRms"
            }
        }
    },
    "suggest": {
        "illuminate/support": "Required for Laravel integration (^11.0|^12.0)"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
